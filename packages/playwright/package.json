{
  "name": "@usedapp/playwright",
  "version": "0.0.1",
  "private": true,
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc",
    "lint": "pnpm lint:prettier --check && pnpm lint:eslint",
    "lint:fix": "prettier --write ./src && pnpm lint:eslint --fix",
    "lint:eslint": "eslint './{src,test}/**/*.{ts,tsx}'",
    "lint:prettier": "prettier --check ./src",
    "postinstall": "unzip -n metamask-chrome-11.15.4.zip -d ./metamask-chrome-11.15.4 && pnpm exec playwright install"
  },
  "dependencies": {
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/node": "^17.0.10",
    "eslint": "7.22.0",
    "playwright": "^1.44.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.2"
  }
}
